[run]
source = hexapod
omit = 
    */tests/*
    */test_*
    */scripts/*
    */__pycache__/*
    */venv/*
    */env/*
    */__init__.py

[report]
show_missing = True
fail_under = 80
exclude_lines =
    pragma: no cover
    if TYPE_CHECKING:
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod